{
  "name": "@app-fe/<%= production %>-<%= type %>-<%= name %>",
  "remoteManifest": {
    "displayName": {
      "none": "",
      "zh_CN": "",
      "en_US": ""
    },
    "type": "<%= type %>",
    "production": "<%= production %>",
    "icon": "icon.svg",
    "port": <%= port %>
  },
  "version": "1.0.0",
  "description": "<%= production %>-<%= type %>-<%= name %>",
  "author": "xiaoYown",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lang": "langm start --folder locales --name <%= production %>-<%= type %>-<%= name %>",
    "start": "pm2 start --name <%= production %>-<%= type %>-<%= name %> npm -- run dev",
    "stop": "pm2 delete <%= production %>-<%= type %>-<%= name %>",
    "dev": "remote-compile start",
    "clean": "del dist",
    "preview": "remote-compile preview",
    "build": "remote-compile build"
  },
  "devDependencies": {
    "@app-fe/remote-compile": "workspace:^1.0.0",
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^27.0.2",
    "@types/node": "18.11.5",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/styled-components": "^5.1.26",
    "babel-loader": "^8.3.0",
    "commitizen": "^4.2.4",
    "del-cli": "5.0.0",
    "typescript": "4.8.4"
  },
  "browserslist": [
    "chrome >= 30",
    "firefox >= 30",
    "safari >= 7",
    "ios >= 9",
    "android >=4"
  ],
  "dependencies": {
    "@app-fe/global-resources": "workspace:^0.0.0",
    "@app-fe/remote-adapter": "workspace:^0.0.0",
    "antd": "4.24.8",
    "i18next": "22.0.6",
    "mobx": "6.7.0",
    "mobx-react-lite": "3.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.0.0",
    "styled-components": "^5.3.6"
  }
