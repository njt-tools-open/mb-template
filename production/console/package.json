{
  "name": "@app-fe/console",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "ts-node --project ./scripts/tsconfig.json -r ./node_modules/tsconfig-paths/register ./scripts/prebuild",
    "build": "pnpm prebuild && tsc && vite build",
    "dev": "pnpm prebuild && vite",
    "lang": "langm start --folder locales --name @app-fe/console",
    "preview": "pnpm build && vite preview",
    "start": "pm2 start --name @app-fe/console 'npm run dev'",
    "stop": "pm2 delete @app-fe/console"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@app-fe/console-storage": "workspace:^0.0.1",
    "@app-fe/global-resources": "workspace:^0.0.1",
    "@app-fe/listener": "workspace:^0.0.1",
    "@app-fe/remote-adapter": "workspace:^0.0.1",
    "antd": "4.24.8",
    "axios": "^1.3.4",
    "i18next": "22.0.6",
    "mobx": "6.7.0",
    "mobx-react-lite": "3.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.0.0",
    "react-router-dom": "^6.4.3"
  },
  "devDependencies": {
    "@app-fe/console-types": "workspace:^1.0.1",
    "@app-fe/types": "workspace:^1.0.1",
    "@types/node": "18.11.5",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/systemjs": "6.13.0",
    "@vitejs/plugin-react": "^2.2.0",
    "del-cli": "5.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "tslib": "2.3.1",
    "typescript": "4.8.4",
    "vite": "^3.2.3",
    "vite-plugin-externals": "^0.5.1"
  }
}
