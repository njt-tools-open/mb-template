{
  "name": "@app-fe/console-layout-app-bar",
  "remoteManifest": {
    "displayName": "应用程序栏",
    "type": "layout",
    "production": "console",
    "icon": "icon.svg",
    "port": 8110
  },
  "version": "1.0.0",
  "description": "应用顶部程序栏",
  "author": "xiaoYown",
